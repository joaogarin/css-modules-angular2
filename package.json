    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "core-js": "^1.2.6",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "0.5.10"
  },
