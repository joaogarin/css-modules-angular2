{
  "name": "angular2-webpack-starter",
  "version": "0.0.0",
  "description": "An Angular 2 Webpack Starter kit featuring Angular 2 (Router, Http, Forms, Services, Tests, E2E), Karma, Protractor, Jasmine, TypeScript, and Webpack by AngularClass",
  "main": "",
  "scripts": {
    "build": "npm run webpack --colors --display-error-details --display-cached",
    "webpack": "webpack",
    "clean": "rimraf node_modules doc",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "watch": "webpack --watch",
    "server": "webpack-dev-server --inline --progress --profile --colors --display-error-details --display-cached --port 3000",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angularclass/angular2-webpack-starter.git"
  },
